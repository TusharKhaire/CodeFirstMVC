@model IEnumerable<TestDemoCodeDAL.DAL.Entity.Customer.CustomerDetails>

@{
    ViewBag.Title = "Index";
}

<h2>Display All Customer</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div>
    <input type="button" id="btnShow" value="Show" />
    <input type="text" id="txtName" />
</div>
<table  id="dataTableData" class="table table-bordered table-striped table-hover  table-condensed" style="width: 100%">
   <thead>
       <tr>
           <th>ID</th>
           <th>CustomerName</th>
           <th>Address</th>
           <th>State</th>
           <th>Dist</th>
           <th>MobileNo</th>
           <th>Email</th>
           <th>DateOfBirth</th>
           <th>OpeningBal</th>
           <th>ClosingBal</th>
           <th>CheckBox</th>
           <th>Action</th>
       </tr>
   </thead>
</table>

@section scripts{
    <script src="~/Scripts/Datatables_JS.js"></script>
}
