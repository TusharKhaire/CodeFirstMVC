@model IEnumerable<TestDemoCodeDAL.DAL.Entity.Masters.ItemType>
@{
    ViewBag.Title = "Index";
}
<h2>Item Types</h2><br />
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.TypeName)</th>
   
        <th>@Html.DisplayNameFor(model => model.Details)</th>
        <th></th>
    </tr>
    @foreach (var type in Model)
    {
        <tr>
            <th>
                @Html.DisplayName(type.TypeName)
            </th>
            <th>@Html.DisplayName(type.Details)</th>
            <th>
                @Html.ActionLink("Edit", "Edit",new {id=type.TypeId }) |
                @Html.ActionLink("Delete", "Delete", new { id = type.TypeId })
            </th>
        </tr>
    }
</table>
